CREATE FUNCTION IF NOT EXISTS timestampToString (input bigint)
CALLED ON NULL INPUT RETURNS text
LANGUAGE java AS '
	java.text.SimpleDateFormat formatter = new java.text.SimpleDateFormat("hh:mm:ss");
	java.util.Date time=new java.util.Date(input);
	return formatter.format(time);
;


CREATE FUNCTION IF NOT EXISTS deltaTime (input bigint) CALLED ON NULL 
INPUT RETURNS bigint LANGUAGE java AS ' long timestamp = 
System.currentTimeMillis(); long diffInMillies = timestamp - input; 
return diffInMillies;';

